<li class="repo" id="<%= full_name %>">

  <a href="<%= url %>">
    <%= name %>
  </a>
  
  <span class="language label">
    <%= language %>
  </span>
  
  <span class="forks"><i class="icon-random"></i>
    <%= forks %>
  </span>
  
  <span class="watchers"><i class="icon-eye-open">
    <%= watchers %>
  </i>
  
</li>